<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/seatBackgroundDrawable">
        <shape android:shape="rectangle">
            <corners android:radius="2dp"/>
            <solid android:color="@color/americanDisabled"/>
        </shape>
    </item>
    <item android:id="@+id/seatBackgroundDrawableSelected">
        <shape android:shape="rectangle">
            <corners android:radius="2dp"/>
            <stroke
                android:width="3dp"
                android:color="@color/black" />
        </shape>
    </item>
</layer-list>